{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "src-DELETE",
    "resolveJsonModule": true
  },
  "include": ["src-DELETE/**/*"],
  "exclude": ["node_modules", "dist"]
} 